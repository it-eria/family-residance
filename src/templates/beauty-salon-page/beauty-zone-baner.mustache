{{#beauty-zone-baner}}
  <section class="banner {{class}}" style="background-image: url(assets/img/{{slide-name}}.jpg);">
    <div class="container">
      <div class="row justify-content-center">
        <div class="col-12 col-lg-10 justify-content-center">
          {{#title}}
            <h2>{{title}}</h2>
          {{/title}}
          {{#desc}}
            <p>{{desc}}</p>
          {{/desc}}
        </div>
      </div>
    </div>
  </section>
{{/beauty-zone-baner}}