{{#price}}
  <section class="content-line content-line__price" style="background-image: url(assets/img/hotel-photo-2.jpg);">
    <div class="container">
      <div class="white-block">
        <div class="inner-block">
          <div class="row">
            <div class="col-12 col-lg-4">
                <div class="section-title ml-3">
                  <h2>Манікюр</h2>
                </div>
                <ul class="item-list">
                    {{#items}}
                  <li>
                    <span>{{{name}}}</span><span>{{{price}}}</span>
                    {{#sublist}}
                    <ul class="item-list__sublist">
                      {{#sub-items}}
                        <li>
                          <span>{{{name}}}</span><span>{{{price}}}</span>
                        </li>
                      {{/sub-items}}
                    </ul>
                    {{/sublist}}
                  </li>  
                  {{/items}}
                </ul>
            </div>
            <div class="col-12 col-lg-4">
              <div class="row mt-5 mt-lg-0">
                <div class="col-12">
                  <div class="section-title">
                    <h2>Педикюр</h2>
                  </div>
                  <ul class="item-list__ln">
                      {{#items-1}}
                    <li>
                      <span>{{{name}}}</span><span>{{{price}}}</span>
                      {{#sublist}}
                      <ul class="item-list__sublist">
                        {{#sub-items}}
                          <li>
                            <span>{{{name}}}</span><span>{{{price}}}</span>
                          </li>
                        {{/sub-items}}
                      </ul>
                      {{/sublist}}
                    </li>  
                    {{/items-1}}
                  </ul>
                </div>
              </div>
              <div class="row mt-price">
                <div class="col-12">
                  <div class="section-title">
                    <h2>Перукарські послуги</h2>
                  </div>
                  <ul class="item-list__ln">
                      {{#items-2}}
                    <li>
                      <span>{{{name}}}</span><span>{{{price}}}</span>
                      {{#sublist}}
                      <ul class="item-list__sublist">
                        {{#sub-items}}
                          <li>
                            <span>{{{name}}}</span><span>{{{price}}}</span>
                          </li>
                        {{/sub-items}}
                      </ul>
                      {{/sublist}}
                    </li> 
                    {{/items-2}} 
                  </ul>
                </div>
              </div>
            </div>
            <div class="col-12 col-lg-4">
              <div class="row mt-5 mt-lg-0">
                <div class="col-12">
                  <div class="section-title">
                    <h2>Бровки</h2>
                  </div>
                  <ul class="item-list__ln">
                      {{#items-3}}
                    <li>
                      <span>{{{name}}}</span><span>{{{price}}}</span>
                      {{#sublist}}
                      <ul class="item-list__sublist">
                        {{#sub-items}}
                          <li>
                            <span>{{{name}}}</span><span>{{{price}}}</span>
                          </li>
                        {{/sub-items}}
                      </ul>
                      {{/sublist}}
                    </li>  
                    {{/items-3}}
                  </ul>
                </div>
              </div>
              <div class="row mt-price-1">
                <div class="col-12">
                  <div class="section-title">
                    <h2>Макіяж</h2>
                  </div>
                  <ul class="item-list__ln">
                      {{#items-4}}
                    <li>
                      <span>{{{name}}}</span><span>{{{price}}}</span>
                      {{#sublist}}
                      <ul class="item-list__sublist">
                        {{#sub-items}}
                          <li>
                            <span>{{{name}}}</span><span>{{{price}}}</span>
                          </li>
                        {{/sub-items}}
                      </ul>
                      {{/sublist}}
                    </li>  
                    {{/items-4}}
                  </ul>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
{{/price}}